def calc(gg):
        if gg == "+":
            p1 = int(input("1 число : "))
            p2 = int(input("2 число : "))
            print("Ответ :", p1 + p2)

        elif gg == "-":
            p1 = int(input("1 число : "))
            p2 = int(input("2 число : "))
            print("Ответ :", p1 - p2)

        elif gg == "*":
            p1 = int(input("1 число : "))
            p2 = int(input("2 число : "))
            print("Ответ :", p1 * p2)

        elif gg == "/":
            p1 = int(input("1 число : "))
            p2 = int(input("2 число : "))
            print("Ответ :", p1 / p2)
